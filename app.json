{
  "expo": {
    "name": "Vibexa",
    "slug": "vibe-exchange",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/vibexa2.png",
    "scheme": "vibexa",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/vibexa2.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.mrnoone.vibeexchange",
      "runtimeVersion": "1.0.0"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/vibexa.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "updates": {
      "url": "https://u.expo.dev/dcb9488a-13b4-4e3a-acaf-b62fd1570077"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "09ffee5b-2714-4648-886c-6e4b85964e2e"
      }
    },
    "owner": "mrnoone"
  }
}
